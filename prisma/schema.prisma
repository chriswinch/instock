// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Bike {
    id String @id @default(cuid())
    created DateTime @default(now())
    title String
    type String
    store String
    price Int
    priceAsString String
    photo String
    link String
}
